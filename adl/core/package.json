    "@azure-tools/uri": "~4.0.0",
    "source-map-support": "0.5.19",
    "ts-morph": "~7.1.0",
    "yaml": "~1.9.2",
    "npm-package-arg": "~8.0.1",
    "semver": "~7.3.2",
    "pacote": "~11.1.10",
    "npm-normalize-package-bin": "~1.0.1",
    "safe-buffer": "~5.2.1"
  }
